{
  "name": "course-buddy-ai",
  "displayName": "Course buddy ai",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.16",
    "@mui/styled-engine-sc": "^6.0.0-alpha.4",
    "plasmo": "^0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.17.0",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.245",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "identity",
      "identity.email",
      "windows"
    ],
    "web_accesible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "tabs/chatbox.html"
        ]
      }
    ]
  }
}
